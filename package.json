{
  "name": "appium-ares",
  "version": "0.0.1",
  "description": "Command line interface for webOS TV",
  "main": "./build/index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "prepare": "gulp prepublish",
    "test": "gulp once && npm run test:types",
    "test:types": "tsc --lib es6,dom --esModuleInterop --noEmit ./typings/index.*.ts",
    "watch": "gulp watch",
    "build": "gulp transpile",
    "mocha": "mocha",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-test": "REPORTER=dot gulp once",
    "lint": "gulp eslint",
    "lint:fix": "gulp eslint --fix",
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sharkyStudy/appium-ares-master.git"
  },
  "author": "Sharky Study <sharky.study@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sharkyStudy/appium-ares-master/issues"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "files": [
    "typings/index.d.ts",
    "index.js",
    "lib",
    "build/index.js",
    "build/lib"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@base2/pretty-print-object": "^1.0.0",
    "appium-support": "^2.48.0",
    "asyncbox": "^2.6.0",
    "child_process": "^1.0.2",
    "lodash": "^4.17.11",
    "shell-quote": "^1.6.1",
    "source-map-support": "^0.5.19",
    "teen_process": "^1.15.0",
    "uuid": "^8.3.2"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit-test"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.138",
    "appium-gulp-plugins": "^5.5.0",
    "appium-test-support": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.23.0",
    "eslint-config-appium": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-promise": "^5.2.0",
    "gulp": "^4.0.0",
    "mocha": "^8.3.2",
    "pre-commit": "^1.1.3",
    "chai": "^4.0.0",
    "chai-as-promised": "^7.1.1",
    "typescript": "^4.2.4"
  },
  "homepage": "https://github.com/sharkyStudy/appium-ares#readme",
  "keywords": [
    "client",
    "interface",
    "appium",
    "webOS"
  ]
}
